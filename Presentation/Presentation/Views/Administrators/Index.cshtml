@model IEnumerable<Service.Models.User>
@{ 
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
  
<h2>ADMINISTRATORS</h2>
<div style="text-align: center;">
    @foreach (var admin in Model)
    {
        <div style="display:inline-flex; font-variant-caps:all-small-caps; font-size: 20px; padding-top: 20px;">@admin.UserName</div>
        if (Session["user"] != null)
        {
            if ((int)Session["userRank"] == 1)
            {
                <form method="post" action="/Administrators/Delete?id=@admin.UserId">
                    <input type="submit" value="Delete admin" class="btn cancelBtn" />
                </form>
            }
        }
    }
</div>