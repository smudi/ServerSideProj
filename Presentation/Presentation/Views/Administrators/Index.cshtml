@model IEnumerable<Service.Models.Administrator>
@{ 
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
  


<h2>ADMINISTRATORS</h2>

<div class="input-group">
    @using (Html.BeginForm())
    {
        @:Search
        @Html.TextBox("searchBox")
        <a href="#" class="btn btn-default">Search</a>
        @*<input type="submit" value="search"/>*@

    }
</div>

<ul>
    @foreach (var admin in Model)
    {
        <li><a>@Html.ActionLink(admin.Name, "Info", "Administrators", new { id = admin.Id }, null)</a></li>
    }
</ul>
@Html.ActionLink("Create Administrator", "Create", "Administrators")
